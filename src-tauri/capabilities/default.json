{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "fs:default",
    "store:default",
    "process:default",
    "log:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:8000" },
        { "url": "http://localhost:8000/*" },
        { "url": "https://localhost:8000" },
        { "url": "https://localhost:8000/*" },
        { "url": "https://*.labric.co" },
        { "url": "https://*.labric.co/*" }
      ]
    }
  ]
}
